<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="573.0" prefWidth="1089.0" styleClass="backgroundC" stylesheets="@Main.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.ticketbar2.gui.controllers.CoordinatorController">
   <right>
      <VBox BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="400.0" />
         </BorderPane.margin>
      </VBox>
   </right>
   <center>
      <FlowPane fx:id="contentPane" prefHeight="486.0" prefWidth="908.0" BorderPane.alignment="TOP_LEFT" />
   </center>
   <top>
      <HBox alignment="TOP_RIGHT" prefHeight="50.0" prefWidth="1090.0" styleClass="eventsC" stylesheets="@Main.css" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="addEvents" mnemonicParsing="false" prefHeight="32.0" prefWidth="45.0" styleClass="buttons" stylesheets="@Main.css">
               <graphic>
                  <ImageView fitHeight="38.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@pictures/icons8-plus-40.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="editEvents" alignment="TOP_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" prefHeight="30.0" prefWidth="31.0" styleClass="buttons" stylesheets="@Main.css">
               <graphic>
                  <ImageView fitHeight="31.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@pictures/pencil.png" />
                     </image>
                     <cursor>
                        <Cursor fx:constant="SW_RESIZE" />
                     </cursor>
                  </ImageView>
               </graphic></Button>
            <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#logout" prefHeight="51.0" prefWidth="107.0" styleClass="buttons" stylesheets="@Main.css" text="Log out">
               <graphic>
                  <ImageView fitHeight="43.0" fitWidth="66.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@pictures/icons8-logout-96.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
   </top>
</BorderPane>
